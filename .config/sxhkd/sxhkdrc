super + F6
	brightnessctl set +1

super + F5
	brightnessctl set 1-

super + F2
	pamixer --increase 5

super + F1
	pamixer --decrease 5

F1
    /home/ezzobir/scripts/run_music.sh
F2
    pkill vlc

XF86AudioPlay
    playerctl --player=vlc play-pause

XF86AudioPrev
    playerctl --player=vlc previous

XF86AudioNext
    playerctl --player=vlc next

