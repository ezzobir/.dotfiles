super + F1

    DISPLAY=:0 feh --no-fehbg --bg-fill --randomize ~/Pictures/wallpapers/*.{png,jpg}

super + F4
	pamixer --decrease 5

super + F5
	pamixer --increase 5

super + F6
	brightnessctl set 1-

super + F7
	brightnessctl set +1

super + F8
    mpd &

super + F9
    killall mpd

super + F10
    playerctl --player=mpd play-pause


XF86AudioPlay
    playerctl play-pause

XF86AudioPrev
    playerctl previous

XF86AudioNext
    playerctl next

